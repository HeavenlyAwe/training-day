;; Anything you type in here will be executed
;; immediately with the results shown on the
;; right.

(+ 2 3)

(+ (* 2 3) 4)

(+ 3 4 5 6)

(get "Clojure" 0)


((fn [who] (str "Hello, " who "!")) "Toffe")



((fn [name] (str "Welcome to Rivendell mr " name)) "Toffe")



(def hello (fn [who] (str "Hello, " who "!")))

(hello "toffe")

(hello "jani")
